<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cooking.mapper.LoginMapper">

  <select id="findAccount" resultType="cooking.dto.Account" parameterType="String">
    SELECT
      UserName,
      UserPwd
    FROM
      UserInfo
    WHERE
      UserName = #{uName}
  </select>

</mapper>
